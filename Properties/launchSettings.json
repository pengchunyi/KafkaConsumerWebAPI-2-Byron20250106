{
	"$schema": "https://json.schemastore.org/launchsettings.json",
	"iisSettings": {
		"windowsAuthentication": false,
		"anonymousAuthentication": true,
		"iisExpress": {
			"applicationUrl": "http://localhost:5000",
			"sslPort": 44350
		}
	},
	"profiles": {
		"KafkaConsumerWebAPI": {
			"commandName": "Project",
